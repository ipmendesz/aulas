// C++ code
//
void setup()
{
  pinMode(13, OUTPUT);
  pinMode(11, OUTPUT);
}

void loop()
{
  PiscaLedVermelho13();
  PiscaLedVermelho11();
}
  
void PiscaLedVermelho13()
 { 
  digitalWrite(13, HIGH);
  delay(100); 
  digitalWrite(13, LOW);
  delay(100); 
}

void PiscaLedVermelho11()
 { 
  digitalWrite(11, HIGH);
  delay(100); 
  digitalWrite(11, LOW);
  delay(100); 
}
